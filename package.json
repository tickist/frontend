{
    "name": "tickist",
    "version": "1.0.0-beta.5",
    "license": "GPL-3.0-or-later",
    "repository": "https://github.com/tickist/frontend",
    "main": "dist/apps/tickist-functions/main.js",
    "engines": {
        "node": "10"
    },
    "scripts": {
        "ng": "ng",
        "nx": "nx",
        "start": "ng serve -c dev --host 0.0.0.0",
        "build": "ng build",
        "test": "ng test",
        "lint": "nx workspace-lint && ng lint",
        "e2e": "npm run build:testConfig && ng e2e",
        "e2e:watch": "npm run build:testConfig && ng e2e --watch ",
        "deploy:development": "ng build && firebase deploy --project development",
        "deploy:staging": "ng build --configuration staging && firebase deploy --project staging",
        "deploy:production": "ng build --prod && firebase deploy --production",
        "firebase:serve": "concurrently --kill-others \"npm run build tickist-functions --watch\" \"firebase serve --only functions,firestore\"",
        "firebase:shell": "concurrently --kill-others \"npm run build tickist-functions --watch\" \"firebase functions:shell\" --raw",
        "deploy:development:functions": "concurrently --kill-others \"npm run build tickist-functions --watch\" \"firebase deploy --project development --only functions\"",
        "affected:apps": "nx affected:apps",
        "affected:libs": "nx affected:libs",
        "affected:build": "nx affected:build",
        "affected:e2e": "nx affected:e2e",
        "affected:test": "nx affected:test",
        "affected:lint": "nx affected:lint",
        "affected:dep-graph": "nx affected:dep-graph",
        "affected": "nx affected",
        "format": "nx format:write",
        "format:write": "nx format:write",
        "format:check": "nx format:check",
        "update": "ng update @nrwl/workspace",
        "update:check": "ng update",
        "workspace-schematic": "nx workspace-schematic",
        "dep-graph": "nx dep-graph",
        "help": "nx help",
        "generate-git-version": "node grab-git-info.js",
        "build:testConfig": "cypress-firebase createTestEnvFile && mv cypress.env.json apps/tickist-web-e2e/"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^8.0.0",
        "@angular/cdk": "~8.0.2",
        "@angular/common": "^8.0.0",
        "@angular/compiler": "^8.0.0",
        "@angular/core": "^8.0.0",
        "@angular/fire": "^5.2.1",
        "@angular/flex-layout": "~8.0.0-beta.26",
        "@angular/forms": "^8.0.0",
        "@angular/material": "~8.0.2",
        "@angular/platform-browser": "^8.0.0",
        "@angular/platform-browser-dynamic": "^8.0.0",
        "@angular/pwa": "^0.13.8",
        "@angular/router": "^8.0.0",
        "@angular/service-worker": "^8.0.0",
        "@auth0/angular-jwt": "^2.1.0",
        "@fortawesome/angular-fontawesome": "^0.4.0",
        "@fortawesome/fontawesome-free": "^5.8.1",
        "@fortawesome/fontawesome-svg-core": "^1.2.19",
        "@fortawesome/free-regular-svg-icons": "^5.9.0",
        "@fortawesome/free-solid-svg-icons": "^5.9.0",
        "@ngrx/effects": "^8.0.1",
        "@ngrx/entity": "^8.0.1",
        "@ngrx/router-store": "^8.0.1",
        "@ngrx/store": "^8.0.1",
        "@ngrx/store-devtools": "^8.0.1",
        "@nrwl/angular": "8.5.1",
        "angular-in-memory-web-api": "0.8.0",
        "chart.js": "^2.8.0",
        "core-js": "^2.5.4",
        "date-fns": "^2.0.1",
        "date-fns-timezone": "^0.1.4",
        "firebase": "^6.3.1",
        "firebase-admin": "^8.3.0",
        "firebase-functions": "^3.2.0",
        "hammerjs": "^2.0.8",
        "jq": "^1.7.2",
        "lodash": ">=4.17.11",
        "logrocket": "^0.6.20",
        "logrocket-ngrx": "^0.1.1",
        "ng2-charts": "^1.5.0",
        "ngx-sortablejs": "^3.1.3",
        "primeng": "^8.0.1",
        "quill": "^1.3.6",
        "rxjs": "^6.5.2",
        "sortablejs": "^1.9.0",
        "truncate-html": "^1.0.2",
        "tsify": "^4.0.1",
        "zone.js": "^0.9.1"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^0.803.3",
        "@angular/cli": "8.3.3",
        "@angular/compiler-cli": "^8.0.0",
        "@angular/language-service": "^8.0.0",
        "@ngrx/schematics": "^8.0.1",
        "@nrwl/cypress": "8.5.1",
        "@nrwl/jest": "8.5.1",
        "@nrwl/node": "8.5.1",
        "@nrwl/workspace": "8.5.1",
        "@types/faker": "^4.1.4",
        "@types/jest": "24.0.9",
        "@types/node": "~8.9.4",
        "codelyzer": "~5.0.1",
        "concurrently": "^4.1.2",
        "cypress": "3.4.1",
        "cypress-cucumber-preprocessor": "^1.16.2",
        "cypress-firebase": "^0.3.0",
        "dotenv": "6.2.0",
        "eslint": "6.1.0",
        "faker": "^4.1.0",
        "firebase-functions-test": "^0.1.6",
        "firebase-tools": "^6.10.0",
        "git-describe": "^4.0.4",
        "husky": "^1.3.1",
        "jest": "24.1.0",
        "jest-preset-angular": "7.0.0",
        "ng-mocks": "8.0.0",
        "prettier": "1.16.4",
        "rxjs-tslint-rules": "^4.14.3",
        "ts-jest": "24.0.0",
        "ts-node": "~7.0.0",
        "tslint": "~5.11.0",
        "typescript": "~3.4.5"
    },
    "cypress-cucumber-preprocessor": {
        "nonGlobalStepDefinitions": true
    }
}
